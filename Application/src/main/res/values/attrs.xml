<resources>
    <declare-styleable name="CameraPreference">
        <attr name="key" format="string" />
        <attr name="title" format="string" />
        <attr name="icon" format="reference" />
        <attr name="generalgroup" format="string" />
        <attr name="uigroup" format="string" />
        <attr name="logicalgroup" format="string" />
    </declare-styleable>

    <declare-styleable name="ProgressPreference">
        <attr name="defaultValue"/>
    </declare-styleable>

    <declare-styleable name="ListPreference">
        <attr name="defaultValue" format="string|reference" />
        <attr name="entryValues" format="reference" />
        <attr name="entries" format="reference" />
    </declare-styleable>
    <declare-styleable name="IconIndicator">
        <attr name="icons" format="reference" />
        <attr name="modes" format="reference" />
    </declare-styleable>
    <declare-styleable name="IconListPreference">
        <!-- If a preference does not have individual icons for each entry, it can has a single icon to represent it. -->
        <attr name="singleIcon" format="reference" />
        <attr name="icons" />
        <attr name="largeIcons" format="reference" />
        <attr name="images" format="reference" />
    </declare-styleable>
    <declare-styleable name="MaxLinearLayout">
        <attr name="maxHeight" format="dimension" />
        <attr name="maxWidth" format="dimension" />
    </declare-styleable>
    <declare-styleable name="Panel">
        <!-- Defines panel animation duration in ms. -->
        <attr name="animationDuration" format="integer" />
        <!-- Defines panel position on the screen. -->
        <attr name="position">
            <!-- Panel placed at top of the screen. -->
            <enum name="top" value="0" />
            <!-- Panel placed at bottom of the screen. -->
            <enum name="bottom" value="1" />
            <!-- Panel placed at left of the screen. -->
            <enum name="left" value="2" />
            <!-- Panel placed at right of the screen. -->
            <enum name="right" value="3" />
        </attr>
        <!-- Identifier for the child that represents the panel's handle. -->
        <attr name="handle" format="reference" />
        <!-- Identifier for the child that represents the panel's content. -->
        <attr name="content" format="reference" />
        <!-- Defines if flying gesture forces linear interpolator in animation. -->
        <attr name="linearFlying" format="boolean" />
        <!-- Defines size relative to parent (must be in form: nn%p). -->
        <attr name="weight" format="fraction" />
        <!-- Defines opened handle (drawable/color). -->
        <attr name="openedHandle" format="reference|color" />
        <!-- Defines closed handle (drawable/color). -->
        <attr name="closedHandle" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="Circle">
        <attr name="circleBackground" format="integer" />
        <attr name="firstChildPosition">
            <enum name="East" value="0" />
            <enum name="South" value="90" />
            <enum name="West" value="180" />
            <enum name="North" value="270" />
        </attr>
        <attr name="rotateToCenter" format="boolean" />
        <attr name="isRotating" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SUI_CircleImageView">
        <attr name="name" format="string" />
    </declare-styleable>

    <declare-styleable name="scenemode">
        <attr name="id" format="integer" />
        <attr name="modeicon" format="reference" />
        <attr name="modeiconfocus" format="reference" />
        <attr name="left" format="dimension" />
        <attr name="top" format="dimension" />
        <attr name="width" format="dimension" />
        <attr name="height" format="dimension" />
        <attr name="rotation" format="integer" />
        <attr name="centerx" format="integer" />
        <attr name="centery" format="integer" />
        <attr name="centerz" format="integer" />
        <attr name="mask" format="reference" />
        <attr name="front" format="reference" />
    </declare-styleable>

    <attr name="btn_photo" format="reference"></attr>
    <attr name="btn_video_start" format="reference"></attr>
    <attr name="btn_video_stop" format="reference"></attr>
    <attr name="btn_video_pause" format="reference"></attr>
    <attr name="btn_video_resum" format="reference"></attr>
    <attr name="btn_thunbnail" format="reference"></attr>
    <attr name="btn_panorama_stop" format="reference"></attr>
    <attr name="btn_video_prepare" format="reference"></attr>
    <attr name="btn_video_prepare_stop" format="reference"></attr>
    <attr name="btn_video_shutter_photo_right" format="reference"></attr>
    <attr name="btn_switch_camera_facing" format="reference"></attr>

    <!--<attr name="ic_index_normal" format="reference"></attr>
    <attr name="index_selected" format="reference"></attr>
    <attr name="index_noselected" format="reference"></attr>-->
    <attr name="index_bottom_margin" format="dimension" />
    <attr name="pref_camera_module_entries" format="reference" />


    <!-- for setting page wiko -->
    <attr name="pref_shutter_touch_icons" format="reference" />
    <attr name="camera_id_icons" format="reference" />
    <attr name="camera_flashmode_icons" format="reference" />
    <attr name="camera_screenflash_icons" format="reference" />
    <attr name="pref_shutter_hand_icons" format="reference" />
    <attr name="pref_shutter_smile_icons" format="reference" />
    <attr name="camera_selftimer_icons" format="reference" />
    <attr name="pref_camera_shutter_sound_icons" format="reference" />
    <attr name="pref_camera_water_mark_icons" format="reference" />
    <attr name="pref_camera_status_detect_icons" format="reference" />
    <attr name="pref_camera_raw_file_icons" format="reference" />
    <attr name="pref_camera_filter_icons" format="reference" />
    <attr name="pref_dual_camera_icons" format="reference" />
    <attr name="pref_camera_recordlocation_icons" format="reference" />
    <attr name="pref_capture_mirror_icons" format="reference" />
    <attr name="pref_pic_struct_icons" format="reference" />
    <attr name="ic_picture_size_icons" format="reference" />
    <attr name="video_quality_icons" format="reference" />
    <attr name="pref_camera_save_pos_icons" format="reference" />
    <attr name="pref_volume_icons" format="reference" />
    <attr name="pref_camera_zsd_icons" format="reference" />
    <attr name="pref_chroma_flash_icons" format="reference" />
    <attr name="pref_camera_recordaudio_icons" format="reference" />
    <attr name="pref_camera_antibanding_icons" format="reference" />
    <attr name="pref_continuous_onoff_icons" format="reference" />
    <attr name="pref_camera_asd_icons" format="reference" />


    <declare-styleable name="view_gravity">
        <attr name="vposition" format="enum">
            <enum name="START" value="0" />
            <enum name="START_LEFT" value="1" />
            <enum name="START_LEFT_RESERVED" value="2" />
            <enum name="CENTER" value="3" />
            <enum name="END_RIGHT_RESERVED" value="4" />
            <enum name="END_RIGHT" value="5" />
            <enum name="END" value="6" />
        </attr>
    </declare-styleable>

    <declare-styleable name="mode_bar">
        <attr name="bgColor" format="color"/>
        <attr name="focusColor" format="color"/>
        <attr name="paddingStart" format="dimension"/>
        <attr name="paddingEnd" format="dimension"/>
        <attr name="insideVerticalOffset" format="dimension"/>
        <attr name="insideHorizonOffset" format="dimension"/>
        <attr name="requireOffset" format="dimension"/>
        <attr name="focusBackground" format="reference"/>
    </declare-styleable>

    <declare-styleable name="button_state">
        <attr name="state_hold_focus" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomScaleBar">
        <attr name="info_text" format="string"/>
        <attr name="right_view_holder_visible" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="FunModeEffect">
        <attr name="effect_id" format="integer"/>
        <attr name="effect_description" format="string|reference"/>
        <attr name="effect_icon" format="reference"/>
        <attr name="effect_asset" format="string"/>
        <attr name="effect_type">
            <enum name="videofx" value="0" />
            <enum name="sticker" value="3" />
        </attr>
        <attr name="effect_face" format="boolean"/>
    </declare-styleable>
</resources>
